{
  "name": "parcel-processor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.701.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "config": "^3.3.1",
    "data-api-client": "^1.0.1",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "forever": "^3.0.0",
    "fs-extra": "^9.0.1",
    "mkdirp": "^1.0.4",
    "ndjson": "^1.5.0",
    "nodemailer": "^6.4.10",
    "rimraf": "^3.0.2",
    "skmeans": "^0.11.3",
    "unzipper": "^0.10.11",
    "uuid": "^8.1.0",
    "winston": "^3.3.0"
  },
  "scripts": {
    "update-site-data-dev": "NODE_ENV=development node site_data_run_from_npm_script.js",
    "update-site-data-prod": "NODE_ENV=production node site_data_run_from_npm_script.js",
    "app-server-test": "NODE_ENV=test node app_run_from_npm_script.js",
    "app-server-dev": "NODE_ENV=development node app_run_from_npm_script.js",
    "app-server-prod": "NODE_ENV=production node app_run_from_npm_script.js",
    "docker-worker-test": "sudo docker run -v $HOME/.aws/credentials:/root/.aws/credentials:ro -d --name test parcel-outlet",
    "worker-test": "NODE_ENV=test node worker_run_from_npm_script.js",
    "worker-dev": "NODE_ENV=development node --max-old-space-size=6144 worker_run_from_npm_script.js",
    "worker-dev-local": "NODE_ENV=development node worker_run_from_npm_script.js",
    "worker-prod": "NODE_ENV=production node --max-old-space-size=6144 worker_run_from_npm_script.js",
    "dangerous-clean-db-dev": "NODE_ENV=development node test/clean-db.js",
    "dangerous-clean-s3-dev": "aws s3 rm s3://raw-data-po-dev --recursive && aws s3 rm s3://data-products-po-dev --recursive && aws s3 rm s3://tile-server-po-dev --recursive && aws s3 rm s3://logfiles-po-dev --recursive",
    "clean-s3-staging": "aws s3 rm s3://staging-po --recursive",
    "loc": "scc --avg-wage 100000 --by-file --exclude-dir test --include-ext js .",
    "build-container": "sudo docker build -t parcel-outlet .",
    "push-ecr": "sudo docker login --username AWS --password `aws ecr get-login-password --region us-east-2` 000009394762.dkr.ecr.us-east-2.amazonaws.com && sudo docker tag parcel-outlet:latest 000009394762.dkr.ecr.us-east-2.amazonaws.com/parcel-outlet:latest && sudo docker push 000009394762.dkr.ecr.us-east-2.amazonaws.com/parcel-outlet:latest"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  }
}
