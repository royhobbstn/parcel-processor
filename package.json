{
  "name": "parcel-processor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.701.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "config": "^3.3.1",
    "data-api-client": "^1.0.1",
    "express": "^4.17.1",
    "forever": "^3.0.0",
    "gdal-next": "^1.0.3",
    "mkdirp": "^1.0.4",
    "ndjson": "^1.5.0",
    "nodemailer": "^6.4.10",
    "rimraf": "^3.0.2",
    "skmeans": "^0.11.3",
    "unzipper": "^0.10.11",
    "uuid": "^8.1.0",
    "winston": "^3.3.0"
  },
  "scripts": {
    "app-server-dev": "NODE_ENV=development node app_run_from_npm_script.js",
    "app-server-prod": "NODE_ENV=production node app_run_from_npm_script.js",
    "worker-local": "NODE_ENV=development node worker_run_from_npm_script.js",
    "worker-dev": "NODE_ENV=development forever start worker_run_from_npm_script.js",
    "worker-prod": "NODE_ENV=production forever start worker_run_from_npm_script.js",
    "dangerous-clean-db-dev": "NODE_ENV=development node test/clean-db.js",
    "dangerous-clean-db-prod": "NODE_ENV=production node test/clean-db.js",
    "dangerous-clean-s3-prod": "aws s3 rm s3://raw-data-po --recursive && aws s3 rm s3://data-products-po --recursive && aws s3 rm s3://tile-server-po --recursive && aws s3 rm s3://logfiles-po --recursive",
    "dangerous-clean-s3-dev": "aws s3 rm s3://raw-data-po-dev --recursive && aws s3 rm s3://data-products-po-dev --recursive && aws s3 rm s3://tile-server-po-dev --recursive && aws s3 rm s3://logfiles-po-dev --recursive",
    "dangerous-clean-s3-staging": "aws s3 rm s3://staging-po --recursive",
    "loc": "scc --by-file --include-ext js .",
    "dangerous-clean-logfiles": "rm -rf staging"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  }
}
